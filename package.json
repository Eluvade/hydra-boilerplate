    "lint": "eslint . src .eslintrc.json --fix"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Eluvade/hydra-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/Eluvade/hydra-boilerplate/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^8.1.0",
    "css-loader": "^5.2.0",
    "css-minimizer-webpack-plugin": "^1.3.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "less": "^4.1.1",
    "less-loader": "^8.0.0",
